
#include <iostream>
using namespace std;
int main() {
    int arr[5];
    int i;
    
    cout<<"Enter numbers:";
    for(i=0;i<5;i++){
        cin>> arr[i];
    }
    
    cout<<"You entered:";
    for(i=0;i<5;i++){
        cout<<arr[i]<<" ";
    }

    return 0;
}

//Output
Enter numbers:70
77
66
55
44
You entered:70 77 66 55 44 
