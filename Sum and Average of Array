
#include <iostream>
using namespace std;
int main() {
    int arr[5]={10,24,56,78,43};
    int sum=0;
    
    for(int i=0;i<5;i++){
        sum=sum+arr[i];
    }
    float average=sum/5;
    cout<<"Sum of array:"<<sum<<endl;
    cout<<"average:"<<average<<endl;
    

    return 0;
}

//Output
Sum of array:211
average:42
