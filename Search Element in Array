// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int arr[5]={10,24,56,78,43};
    int search;
    
    cout<<"Enter number to be searched:";
    cin>>search;
    for(int i=0;i<5;i++){
        if(search==arr[i]){
            cout<<"Element found\n";
            cout<<"Position is "<<i;
            return 0;
            
        }
    }
    cout<<"element not there";
    

    return 0;
}


//Output
Enter number to be searched:43
Element found
Position is 4
